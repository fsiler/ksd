{
    "name": "efficiently/larasset",
    "description": "Larasset is a library for Laravel 5 which manage assets in an easy way.",
    "keywords": [
        "assets",
        "sprockets",
        "mincer",
        "node",
        "asset-pipeline",
        "pipeline",
        "css",
        "javascript",
        "less",
        "sass",
        "scss",
        "coffeescript",
        "minify",
        "precompile",
        "sourcemaps",
        "source-map",
        "manifest",
        "versioning",
        "laravel",
        "laravel 5",
        "laravel 5.1"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tortue Torche",
            "email": "tortuetorche@spam.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravelcollective/html": "~5.0.0||~5.1.0",
        "efficiently/jquery-laravel": "~2.0.0"
    },
    "require-dev": {
        "illuminate/support": "~5.0",
        "illuminate/console": "~5.0"
    },
    "autoload": {
        "classmap": [
        ],
        "files": [
            "src/Efficiently/Larasset/helpers.php"
        ],
        "psr-0": {
            "Efficiently\\Larasset\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
